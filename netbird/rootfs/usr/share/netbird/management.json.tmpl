    "AuthAudience": "${NETBIRD_AUTH_AUDIENCE}",
    "AuthKeysLocation": "${NETBIRD_AUTH_JWT_CERTS}",
    "AuthUserIDClaim": "${NETBIRD_AUTH_USER_ID_CLAIM}",
    "CertFile": "${NETBIRD_MGMT_API_CERT_FILE}",
    "CertKey": "${NETBIRD_MGMT_API_CERT_KEY_FILE}",
