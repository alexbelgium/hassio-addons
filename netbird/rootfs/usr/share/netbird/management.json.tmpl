{
  "Signal": {
    "Proto": "${NETBIRD_SIGNAL_PROTOCOL}",
    "URI": "${NETBIRD_DOMAIN}:${NETBIRD_SIGNAL_PORT}",
    "Username": "",
    "Password": null
  },
  "ReverseProxy": {
    "TrustedHTTPProxies": [],
    "TrustedHTTPProxiesCount": 0,
    "TrustedPeers": [
      "0.0.0.0/0"
    ]
