  "image": "ghcr.io/alexbelgium/organizr-{arch}",
  "boot": "auto",
  "environment": {},
  "options": {
    "PUID": "1000",
    "PGID": "1000"
  },
  "schema": {
    "PUID": "int",
    "PGID": "int"
  }
}
