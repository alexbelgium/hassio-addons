    "PGID": "int",
    "PUID": "int",
    "TZ": "str?",
    "cifsdomain": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "localdisks": "str?",
    "networkdisks": "str?"
  },
  "slug": "booksonic-air_nas",
  "udev": true,
  "url": "https://github.com/alexbelgium/hassio-addons/tree/master/booksonic_air",
  "version": "2201.1.0",
