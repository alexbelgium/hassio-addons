{
  "apparmor": true,
  "arch": ["aarch64", "amd64", "armv7", "armhf"],
  "boot": "auto",
  "description": "scan, index and archive all your physical documents",
  "image": "ghcr.io/alexbelgium/paperless_ng-{arch}",
  "devices": [
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/mmcblk",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "map": ["config:rw", "share:rw", "media:rw", "config:rw"],
  "name": "Paperless NG",
  "options": {
    "PUID": 0,
    "PGID": 0,
    "CONFIG_LOCATION": "/config/addons_config/paperless_ng/config.yaml"
  },
  "ports": {
    "8000/tcp": 8011
  },
  "ports_description": {
    "8000/tcp": "web interface"
  },
  "privileged": ["SYS_ADMIN", "DAC_READ_SEARCH"],
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?",
    "CONFIG_LOCATION": "str",
    "localdisks": "str?",
    "networkdisks": "str?",
    "cifsusername": "str?",
    "cifspassword": "str?",
    "OCRLANG": "str?"
  },
  "slug": "paperless_ng",
  "upstream": "1.5.0",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "1.5.0-8",
  "webui": "http://[HOST]:[PORT:8000]"
}
