{
  "name": "Readarr NAS",
  "version": "nightly-alpine-0.1.0.1154-ls4",
  "slug": "readarr_nas",
  "description": "Book Manager and Automation",
  "url": "https://github.com/alexbelgium/hassio-addons/tree/master/readarr",
  "apparmor": true,
  "arch": ["aarch64", "amd64", "armv7", "armhf"],
  "boot": "auto",
  "upstream": "nightly-alpine-0.1.0.1154-ls4 ",
  "webui": "http://[HOST]:[PORT:8787]",
  "startup": "services",
  "devices": [
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/mmcblk",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "map": [
    "config:rw",
    "share:rw",
    "media:rw",
    "config:rw"
  ],
  "options": {
    "PUID": 0,
    "PGID": 0,
    "CONFIG_LOCATION": "/config/readarr"
  },
  "ports": {
    "8787/tcp": 8787
  },
  "ports_description": {
    "8787/tcp": "web interface"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?",
    "localdisks": "str?",
    "networkdisks": "str?",
    "cifsusername": "str?",
    "cifspassword": "str?",
    "CONFIG_LOCATION": "str"
  }
}
