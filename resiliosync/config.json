  "image": "ghcr.io/alexbelgium/resiliosync-{arch}",
  "ingress": true,
  "init": false,
  "map": [
    "addon_config:rw",
    "backup:rw",
    "homeassistant_config:rw",
    "media:rw",
    "share:rw",
    "ssl:rw"
  ],
  "name": "ResilioSync",
  "options": {
    "PGID": 0,
    "PUID": 0,
    "config_location": "/config/addons_config/resiliosync",
    "data_location": "/share/resiliosync",
    "downloads_location": "/share/resiliosync_downloads"
  },
  "panel_admin": false,
  "panel_icon": "mdi:cloud-sync-outline",
  "ports": {
    "55555/tcp": 55555,
    "8888/tcp": 8888
  },
  "ports_description": {
    "55555/tcp": "Sync Port",
    "8888/tcp": "Web interface"
  },
  "privileged": ["SYS_ADMIN", "DAC_READ_SEARCH", "NET_ADMIN"],
  "schema": {
    "PGID": "int",
    "PUID": "int",
    "TZ": "str?",
    "cifsdomain": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
