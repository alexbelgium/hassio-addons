  "environment": {
    "LANG": "en-US",
    "PGID": "1000",
    "PORT": "80",
    "PRETTY_URLS": "1",
    "PUID": "1000",
    "WEBTREES_HOME": "/var2/www/webtrees"
  },
  "image": "ghcr.io/alexbelgium/webtrees-{arch}",
  "init": false,
  "map": ["addon_config:rw", "homeassistant_config:rw", "share:rw", "ssl"],
  "name": "Webtrees",
  "options": {
    "BASE_URL": "http://192.168.178.23",
    "DATA_LOCATION": "/config/data",
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "ssl": false
  },
  "ports": {
    "443/tcp": null,
    "80/tcp": 9999
  },
  "ports_description": {
    "443/tcp": "Https web interface",
    "80/tcp": "Web interface"
  },
  "privileged": ["SYS_ADMIN", "DAC_READ_SEARCH"],
  "schema": {
    "BASE_URL": "url",
    "DATA_LOCATION": "str",
    "base_url_portless": "bool?",
    "certfile": "str",
    "cifsdomain": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "keyfile": "str",
    "localdisks": "str?",
    "networkdisks": "str?",
    "ssl": "bool",
    "trusted_headers": "str?"
  },
  "services": ["mysql:want"],
  "slug": "webtrees",
  "udev": true,
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "2.2.1-4",
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]"
}
