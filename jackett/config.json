{
  "name": "Jackett NAS",
  "version": "0.20.428",
  "slug": "jackett_nas",
  "description": "translates queries from apps (Sonarr, SickRage, CouchPotato, Mylar, etc) into tracker-site-specific http queries, parses the html response, then sends results back to the requesting software",
  "url": "https://github.com/alexbelgium/hassio-addons/tree/master/jackett",
  "apparmor": true,
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "boot": "auto",
  "upstream": "0.20.428",
  "startup": "services",
  "webui": "http://[HOST]:[PORT:9117]",
  "environment": {
    "PUID": "0",
    "PGID": "0"
  },
  "map": [
    "config:rw",
    "share:rw",
    "media:rw",
    "ssl"
  ],
  "options": {
    "PUID": 0,
    "PGID": 0
  },
  "ports": {
    "9117/tcp": 9117
  },
  "ports_description": {
    "9117/tcp": "web interface"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "PUID": "int",
    "PGID": "int",
    "TZ": "str?",
    "localdisks": "str?",
    "networkdisks": "str?",
    "cifsusername": "str?",
    "cifspassword": "str?"
  }
}
