{
  "name": "TeslaMate ABRP",
  "version": "1.0.0",
  "slug": "teslamate-abrp",
  "description": "A Better Route Planner integration for TeslaMate",
  "url": "https://github.com/fetzu/teslamate-abrp",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "application",
  "boot": "auto",
  "environment": {
    "LOG_LEVEL": "info"
  },
  "services": ["mqtt:want"],
  "schema": {
    "abrp_token": "str",
    "abrp_user_token": "str",
    "mqtt_host": "str",
    "mqtt_port": "port",
    "mqtt_username": "str?",
    "mqtt_password": "password?"
  },
  "ports": {}
}
