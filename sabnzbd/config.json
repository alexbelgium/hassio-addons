{
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "Makes Usenet as simple and streamlined as possible by automating everything we can",
  "devices": [
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/nvme0n1p1",
    "/dev/nvme0n1p2",
    "/dev/mmcblk",
    "/dev/fuse",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {
    "PGID": "0",
    "PUID": "0"
  },
  "image": "ghcr.io/alexbelgium/sabnzbd-{arch}",
  "init": false,
  "map": [
    "config:rw",
    "share:rw",
    "media:rw",
    "config:rw"
  ],
  "name": "Sabnzbd",
  "options": {
    "PGID": 0,
    "PUID": 0
  },
  "panel_icon": "mdi:arrow-down-bold-circle-outline",
  "ports": {
    "8080/tcp": 8089
  },
  "ports_description": {
    "8080/tcp": "web interface"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "PGID": "int",
    "PUID": "int",
    "TZ": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "localdisks": "str?",
    "networkdisks": "str?"
  },
  "slug": "sabnzbd",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "3.7.1-ls89",
  "webui": "http://[HOST]:[PORT:8080]"
}
