{
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "filebrowser provides a file managing interface within a specified directory and it can be used to upload, delete, preview, rename and edit your files",
  "devices": [
    "/dev/dri",
    "/dev/dri/card0",
    "/dev/dri/card1",
    "/dev/dri/renderD128",
    "/dev/vchiq",
    "/dev/video10",
    "/dev/video11",
    "/dev/video12",
    "/dev/video13",
    "/dev/video14",
    "/dev/video15",
    "/dev/video16",
    "/dev/ttyUSB0",
    "/dev/sda",
    "/dev/sdb",
    "/dev/sdc",
    "/dev/sdd",
    "/dev/sde",
    "/dev/sdf",
    "/dev/sdg",
    "/dev/nvme",
    "/dev/nvme0n1p1",
    "/dev/nvme0n1p2",
    "/dev/mmcblk",
    "/dev/fuse",
    "/dev/sda1",
    "/dev/sdb1",
    "/dev/sdc1",
    "/dev/sdd1",
    "/dev/sde1",
    "/dev/sdf1",
    "/dev/sdg1",
    "/dev/sda2",
    "/dev/sdb2",
    "/dev/sdc2",
    "/dev/sdd2",
    "/dev/sde2",
    "/dev/sdf2",
    "/dev/sdg2",
    "/dev/sda3",
    "/dev/sdb3",
    "/dev/sda4",
    "/dev/sdb4",
    "/dev/nvme0",
    "/dev/nvme1",
    "/dev/nvme2"
  ],
  "environment": {
    "FB_BASEURL": "/filebrowser",
    "PGID": "0",
    "PUID": "0"
  },
  "image": "ghcr.io/alexbelgium/filebrowser-{arch}",
  "ingress": true,
  "ingress_stream": true,
  "map": [
    "media:rw",
    "config:rw",
    "addons:rw",
    "backup:rw",
    "share:rw",
    "ssl:rw"
  ],
  "name": "Filebrowser",
  "options": {
    "NoAuth": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "ssl": false
  },
  "panel_icon": "mdi:file-search",
  "ports": {
    "8080/tcp": null
  },
  "ports_description": {
    "8080/tcp": "Web UI port"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "NoAuth": "bool",
    "base_folder": "str?",
    "certfile": "str",
    "cifsdomain": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "keyfile": "str",
    "localdisks": "str?",
    "networkdisks": "str?",
    "smbv1": "bool?",
    "ssl": "bool"
  },
  "slug": "filebrowser",
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "2.23.0-5"
}
