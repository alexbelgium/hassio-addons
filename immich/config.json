    "TZ": "Europe/Paris",
    "data_location": "/share/immich"
  },
  "panel_admin": false,
  "panel_icon": "mdi:file-search",
  "ports": {
    "8080/tcp": 8181
  },
  "ports_description": {
    "8080/tcp": "Web UI port"
  },
  "privileged": [
    "SYS_ADMIN",
    "DAC_READ_SEARCH"
  ],
  "schema": {
    "DB_DATABASE_NAME": "str",
    "DB_HOSTNAME": "str",
    "DB_PASSWORD": "str",
