    "PGID": "int",
    "PUID": "int",
    "TZ": "str?",
    "cifspassword": "str?",
    "cifsusername": "str?",
    "data_location": "str",
    "database": "list(internal|external_postgresql)?",
    "localdisks": "str?",
    "networkdisks": "str?"
  },
  "services": [
    "mysql:want"
  ],
  "slug": "immich",
  "url": "https://github.com/alexbelgium/hassio-addons",
