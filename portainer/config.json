{
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "backup_exclude": [
    "backups",
    "docker_config/cli-plugins"
  ],
  "codenotary": "alexandrep.github@gmail.com",
  "description": "Manage your Docker environment with ease",
  "docker_api": true,
  "hassio_api": true,
  "image": "ghcr.io/alexbelgium/portainer-{arch}",
  "ingress": true,
  "ingress_port": 1337,
  "ingress_stream": true,
  "init": false,
  "map": [
    "addon_config:rw",
    "share:rw",
    "ssl"
  ],
  "name": "Portainer",
  "options": {
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "password": "homeassistant",
    "ssl": false
  },
  "panel_admin": false,
  "panel_icon": "mdi:docker",
  "ports": {
    "8000/tcp": null,
    "9099/tcp": 9000
  },
  "ports_description": {
    "8000/tcp": "Edge Agent Api (Enable when managing remote edge agents)",
    "9099/tcp": "Web UI port"
  },
  "schema": {
    "certfile": "str",
    "keyfile": "str",
    "password": "str?",
    "ssl": "bool"
  },
  "slug": "portainer",
  "udev": true,
  "url": "https://github.com/alexbelgium/hassio-addons",
  "version": "2.32.0-4"
}
